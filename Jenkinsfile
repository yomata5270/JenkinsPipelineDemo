pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello from github webhook trigger'
            }
        }
        
        stage('build') {
            steps {
                echo 'buildiing'
            }
        }
        
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        
        stage('release') {
            steps {
                echo 'releaseing'
            }
        }
    }
}
